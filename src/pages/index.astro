---
import Header from '@/components/Header.astro';
import Proyectos from '@/components/Proyectos.astro';
import Certificaciones from '@/components/Certificaciones.astro';
import AboutMe from '@/components/AboutMe.astro';
import Layout from '@/layouts/Layout.astro';
import Experiencia from '@/components/Experiencia.astro';
const experiencias = [
  {
    puesto: "Desarrollador .NET",
    empresa: "Oesia",
    fecha: "marzo de 2024 - septiembre de 2024",
    descripcion: "Mi principal función fue el mantenimiento y desarrollo de una aplicación de escritorio para la gestión de ElPozo, utilizando tecnologías como Visual Basic, SQL, PL/SQL, Git y DevExpress,además de recibir formación en ASP.NET y Android Studio. "
  },
  {
    puesto: "Operario de linea",
    empresa: "Nueva Cocina Mediterranea",
    fecha: "2019 - 2022",
    descripcion: "Recepción, inspección y almacenamiento adecuado de materias primas y materiales de embalaje.Realización de tareas de corte, picado, mezclado, o lavado de ingredientes según el proceso de producción. Supervisión del buen funcionamiento de las máquinas y ajustes según sea necesario en línea de producción. Manipulación de alimentos en línea de producción"
  }
];

import "@/styles/global.css"
---

<Layout>
  <Header />
  <AboutMe/>
  <Experiencia experiencias={experiencias} />

  <Proyectos />
  <Certificaciones />
  
</Layout>